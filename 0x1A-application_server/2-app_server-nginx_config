#Serve a page with Nginx

server {
	listen 80 default_server;
	listen [::]:80 default_server;
	
	server_name 35.153.67.140;

	add_header X-Served-By 459245-web-01;

	location = /airbnb-onepage/ {
		roxy_pass http://127.0.0.1:5000/airbnb-onepage/;
	}

	error_page 404 /404.html;
        location /404 {
                root /var/www/html;
                internal;
        }
	
}
